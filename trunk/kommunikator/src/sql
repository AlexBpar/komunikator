update ntn_settings set value = 'false' where param = 'incoming_trunk'
insert into ntn_settings (param,value) select 'incoming_trunk', 'false' from dual where not exists (select 1 from ntn_settings where param = 'incoming_trunk' and value = 'false')
update ntn_settings set value = 'false' where param = 'incoming_call'
