update ntn_settings set value = 'true' where param = 'incoming_trunk'
insert into ntn_settings (param,value) select 'incoming_trunk', 'true' from dual where not exists (select 1 from ntn_settings where param = 'incoming_trunk' and value = 'true')
update ntn_settings set value = 'true' where param = 'incoming_call'
insert into ntn_settings (param,value) select 'incoming_call', 'true' from dual where not exists (select 1 from ntn_settings where param = 'incoming_call' and value = 'true')
update ntn_settings set value = 'false' where param = 'outgoing_call'
insert into ntn_settings (param,value) select 'outgoing_call', 'false' from dual where not exists (select 1 from ntn_settings where param = 'outgoing_call' and value = 'false')
update ntn_settings set value = 'false' where param = 'internal_call'
insert into ntn_settings (param,value) select 'internal_call', 'false' from dual where not exists (select 1 from ntn_settings where param = 'internal_call' and value = 'false')
update ntn_settings set value = '' where param = 'email'
insert into ntn_settings (param,value) select 'email', '' from dual where not exists (select 1 from ntn_settings where param = 'email' and value = '')
