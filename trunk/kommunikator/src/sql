update ntn_settings set value = '' where param = 'from'
insert into ntn_settings (param,value) select 'from', '' from dual where not exists (select 1 from ntn_settings where param = 'from' and value = '')
update ntn_settings set value = '' where param = 'fromname'
insert into ntn_settings (param,value) select 'fromname', '' from dual where not exists (select 1 from ntn_settings where param = 'fromname' and value = '')
update ntn_settings set value = 'some@gh.ru' where param = 'email'
insert into ntn_settings (param,value) select 'email', 'some@gh.ru' from dual where not exists (select 1 from ntn_settings where param = 'email' and value = 'some@gh.ru')
